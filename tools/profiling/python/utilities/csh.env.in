set modpath=@CMAKE_INSTALL_PREFIX@/@PARSEC_INSTALL_LIBDIR@/python@Python_VERSION_MAJOR@.@Python_VERSION_MINOR@/site-package
if ( ! -d ${modpath} ) then
  set modpath=@CMAKE_INSTALL_PREFIX@/lib64/python@Python_VERSION_MAJOR@.@Python_VERSION_MINOR@/site-package
  if ( ! -d ${modpath} ) then
    set modpath=@CMAKE_INSTALL_PREFIX@/lib/python@Python_VERSION_MAJOR@.@Python_VERSION_MINOR@/site-packages
    if ( ! -d ${modpath} ) then
      set modpath=""
    endif
  endif
endif

if ( "x${modpath}" != "x" ) then
  if( $?PYTHONPATH) then
    setenv PYTHONPATH "$modpath:$PYTHONPATH"
  else
    setenv PYTHONPATH "$modpath"
  endif
endif
unset modpath

setenv PATH "@CMAKE_INSTALL_PREFIX@/@PARSEC_INSTALL_BINDIR@:$PATH"

