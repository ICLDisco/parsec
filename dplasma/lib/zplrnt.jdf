extern "C" %{
  /**
   * PLASMA include for defined and constants.
   *
   * @precisions normal z -> s d c
   *
   */
#include <plasma.h>
#include <core_blas.h>

#define PRECISION_z
#define BLKLDD(_desc, _k) (_desc).mb

#include "dague.h"
#include "data_distribution.h"
#include "dplasmajdf.h"
#include "data_dist/matrix/matrix.h"

%}

/*
 * Globals
 */
seed   [type = "unsigned long long int" ]
descA  [type = "tiled_matrix_desc_t"]
A      [type = "dague_ddesc_t *"]

PLRNT(m, n)

// Execution space
m = 0 .. descA.mt-1
n = 0 .. descA.nt-1

// Parallel partitioning
: A(m, n)

// Parameters

RW A <- A(m, n)
     -> A(m, n)
BODY
  int tempmm = ((m)==((descA.mt)-1)) ? ((descA.m)-(m*(descA.mb))) : (descA.mb);
  int tempnn = ((n)==((descA.nt)-1)) ? ((descA.n)-(n*(descA.nb))) : (descA.nb);
  int ldam = BLKLDD( descA, m );

        DRYRUN(
               CORE_zplrnt( 
                   tempmm, tempnn, A, ldam,
                   descA.m, m*descA.mb, n*descA.nb, seed );
               );

        printlog("thread %d plrnt( %d, %d )\n"
                 "    ( %d, %d, A(%d,%d), %d, %d, %d, %d, %llu)\n",
                 context->eu_id, m, n, tempmm, tempnn, 
                 m, n, ldam,
                 descA.m, m*descA.mb, n*descA.nb, seed);
END

extern "C" %{

%}
